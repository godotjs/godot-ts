{
  "name": "@godot-js/ts",
  "version": "0.0.0",
  "description": "Tool for using GodotJS with TypeScript",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/godotjs/godot-ts"
  },
  "license": "MIT",
  "bin": {
    "@godot-js/ts": "dist/index.js"
  },
  "main": "index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "node esbuild.js",
    "lint": "eslint *.ts",
    "prepare": "husky",
    "test": "vitest run --no-file-parallelism",
    "test:cli:help": "tsx src/cli.ts --help",
    "test:cli:build": "tsx src/cli.ts build --src .\\test\\esbuild\\src\\ --out .\\test\\esbuild\\scripts",
    "test:cli:watch": "tsx src/cli.ts watch --src ./test/esbuild/src/ --out ./test/esbuild/scripts"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "glob": "^11.0.0",
    "esbuild": "0.24.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.11.1",
    "globals": "^15.9.0",
    "husky": "^9.0.11",
    "prettier": "^3.0.3",
    "tsx": "^4.19.1",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.3",
    "vitest": "^2.0.5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
