{
  "name": "@godot-js/godot-ts",
  "version": "0.0.0",
  "description": "Tool for using GodotJS with TypeScript",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/godotjs/godot-ts"
  },
  "license": "MIT",
  "bin": {
    "@godot-js/godot-ts": "dist/index.js"
  },
  "main": "index.js",
  "files": [
    "dist/"
  ],
  "workspaces": [
    "test"
  ],
  "scripts": {
    "build": "node esbuild.js",
    "generate:api-docs": "tsx src/api-docs.ts",
    "lint": "eslint *.ts",
    "postinstall": "tsx src/utils/prepare.ts",
    "prebuild": "tsx src/utils/prepare.ts",
    "prepare": "husky",
    "pretest": "tsx src/utils/prepare.ts",
    "test": "vitest run --no-file-parallelism"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "commander": "^14.0.0",
    "cosmiconfig": "^9.0.0",
    "esbuild": "0.27.2",
    "glob": "^13.0.0",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.1",
    "find-versions-cli": "^5.0.0",
    "globals": "^16.0.0",
    "husky": "^9.0.11",
    "markdown-table": "^3.0.3",
    "nodemon": "3.1.11",
    "npm-run-all2": "8.0.4",
    "prettier": "^3.0.3",
    "tsx": "^4.19.1",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^7.0.0",
    "vitest": "^3.0.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
